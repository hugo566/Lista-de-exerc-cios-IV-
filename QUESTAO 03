#include <stdio.h>

void insertionSort(float arr[], int n) {
    int i, j;
    float temp;

    for (i = 1; i < n; i++) {
        temp = arr[i];
        j = i - 1;

        while (j >= 0 && arr[j] > temp) {
            arr[j + 1] = arr[j];
            j--;
        }
        arr[j + 1] = temp;
    }
}

int main() {
    int n, i;
    
    printf("Quantidade de produtos: ");
    scanf("%d", &n);
    
    float precos[n];
    
    printf("Digite os preços:\n");
    for (i = 0; i < n; i++) {
        scanf("%f", &precos[i]);
    }

    insertionSort(precos, n);

    printf("Preços ordenados:\n");
    for (i = 0; i < n; i++) {
        printf("%.2f ", precos[i]);
    }
    printf("\n");

    return 0;
}
