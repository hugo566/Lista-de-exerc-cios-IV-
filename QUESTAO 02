#include <stdio.h>

int main() {
    int num, i;

    printf("Digite um número inteiro positivo: ");
    scanf("%d", &num);

    if (num > 1) {
        printf("Números pares entre 1 e %d:\n", num);
        for (i = 2; i <= num; i++) {
            if (i % 2 == 0) {
                printf("%d ", i);
            }
        }
        printf("\n");
    } else {
        printf("Número inválido.\n");
    }

    return 0;
}
